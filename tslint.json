{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "eofline": [
            false
        ],
        "curly": [
            false
        ],
        "trailing-comma": [
            false
        ],
        "object-literal-sort-keys": [
            false
        ],
        "ordered-imports": [
            false
        ],
        "arrow-parens": [
            false
        ],
        "member-access": [
            false
        ],
        "interface-over-type-literal": [
            false
        ],
        "max-line-length": [
            true,
            200
        ]
    },
    "rulesDirectory": []
}